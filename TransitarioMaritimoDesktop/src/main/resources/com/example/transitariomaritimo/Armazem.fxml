<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480" prefWidth="720" stylesheets="@css/nord-light.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.transitariomaritimo.ArmazemController">
   <children>
      <Pane fx:id="mainPanel" prefHeight="715.0" prefWidth="883.0">
         <children>
            <Label layoutX="370.0" layoutY="25.0" text="Armazem">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
             <TableView fx:id="table" layoutX="14.0" layoutY="81.0" prefHeight="431.0" prefWidth="829.0">
                 <columns>
                     <TableColumn fx:id="Id" prefWidth="183.0" text="Id" />
                     <TableColumn fx:id="Descricao" prefWidth="242.0" text="Descricao" />
                     <TableColumn fx:id="Cap_max" prefWidth="201.0" text="Cap_Max" />
                 </columns>
             </TableView>
             <Button defaultButton="true" layoutX="21.0" layoutY="532.0" mnemonicParsing="false" onAction="#InserirArmazem" prefHeight="42.0" prefWidth="109.0" text="Inserir Armazem">
               <font>
                  <Font size="11.0" />
               </font></Button>
             <Button layoutX="150.0" layoutY="532.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="117.0" text="Remover Armazem">
               <font>
                  <Font size="11.0" />
               </font></Button>
             <Button layoutX="287.0" layoutY="532.0" mnemonicParsing="false" onAction="#VisualizarCargas" prefHeight="42.0" prefWidth="117.0" text="Visualizar Cargas">
               <font>
                  <Font size="11.0" />
               </font></Button>
         </children>
      </Pane>

      <Pane fx:id="PaneInserir" layoutX="254.0" layoutY="144.0" prefHeight="380.0" prefWidth="338.0" style="-fx-background-color: #FFFFFF;" visible="false">
         <children>
             <Label layoutX="97.0" layoutY="17.0" text="Inserir Armazem">
             <font>
                 <Font size="18.0" />
             </font>
         </Label>
             <TextField fx:id="descricaoText" layoutX="67.0" layoutY="102.0" prefHeight="26.0" prefWidth="199.0" />
             <TextField fx:id="CapacidadeMaximaText" layoutX="67.0" layoutY="196.0" prefHeight="26.0" prefWidth="199.0" />
             <Button defaultButton="true" layoutX="98.0" layoutY="286.0" mnemonicParsing="false" onAction="#RegistarArmazem" text="Registar Armazem" />
             <Label layoutX="67.0" layoutY="72.0" text="Descrição" />
             <Label layoutX="67.0" layoutY="168.0" text="Capacidade Maxima" />
            <Button layoutX="278.0" layoutY="12.0" mnemonicParsing="false" onAction="#VoltarAtrasInserirArmazem" text="x" />
         </children>
         <effect>
            <DropShadow />
         </effect></Pane>

   </children>
</AnchorPane>
